{
    "name": "dougsisk/laravel-country-state",
    "description": "Country & state helper for Laravel.",
    "keywords": [
        "countries",
        "country",
        "laravel",
        "state",
        "states"
    ],
    "homepage": "https://github.com/DougSisk/laravel-country-state",
    "license": "MIT",
    "authors": [
        {
            "name": "Doug Sisk",
            "homepage": "http://dougsisk.com"
        }
    ],
    "support": {
        "issues": "https://github.com/DougSisk/laravel-country-state/issues"
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "rinvex/countries": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "DougSisk\\CountryState\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DougSisk\\CountryState\\CountryStateServiceProvider"
            ]
        }
    }
}